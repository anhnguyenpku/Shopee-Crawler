{
    "_id": "shopee-v1",
    "startUrl": [
        "https://banhang.shopee.vn/portal/product/list/all?page=[1-4]&size=99&order=popularity_dsc"
    ],
    "selectors": [
        {
            "id": "product",
            "type": "SelectorElement",
            "parentSelectors": [
                "view-type"
            ],
            "selector": "div.product-card-item",
            "multiple": true,
            "delay": 0
        },
        {
            "id": "image",
            "type": "SelectorImage",
            "parentSelectors": [
                "product"
            ],
            "selector": "img",
            "multiple": false,
            "delay": 0
        },
        {
            "id": "title",
            "type": "SelectorText",
            "parentSelectors": [
                "product"
            ],
            "selector": "div.product-name-text",
            "multiple": false,
            "regex": "",
            "delay": 0
        },
        {
            "id": "price",
            "type": "SelectorText",
            "parentSelectors": [
                "product"
            ],
            "selector": "div.price-display",
            "multiple": false,
            "regex": "",
            "delay": 0
        },
        {
            "id": "stock",
            "type": "SelectorText",
            "parentSelectors": [
                "product"
            ],
            "selector": ".text-overflow span:nth-of-type(2)",
            "multiple": false,
            "regex": "",
            "delay": 0
        },
        {
            "id": "view",
            "type": "SelectorText",
            "parentSelectors": [
                "product"
            ],
            "selector": "div:nth-of-type(1) span.product-meta-number",
            "multiple": false,
            "regex": "",
            "delay": 0
        },
        {
            "id": "sale",
            "type": "SelectorText",
            "parentSelectors": [
                "product"
            ],
            "selector": ".product-meta-sales span.product-meta-number",
            "multiple": false,
            "regex": "",
            "delay": 0
        },
        {
            "id": "view-type",
            "type": "SelectorElementClick",
            "parentSelectors": [
                "_root"
            ],
            "selector": "div.product-grid-view",
            "multiple": false,
            "delay": 0,
            "clickElementSelector": "i.grid-view",
            "clickType": "clickOnce",
            "discardInitialElements": "do-not-discard",
            "clickElementUniquenessType": "uniqueText"
        },
        {
            "id": "item-id",
            "type": "SelectorElementAttribute",
            "parentSelectors": [
                "product"
            ],
            "selector": ".shopee-checkbox__input",
            "multiple": false,
            "extractAttribute": "value",
            "delay": 0
        }
    ]
}